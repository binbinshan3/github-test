from typing import List
class Solution:
    def searchMatrix(self, matrix: List[List[int]], target: int) -> bool:
        m=len(matrix)
        n-len(matrix[0])
        row=0
        col=n-1
        # 这个地方的边界条件还挺巧妙的，这个col>=0一定不会越界，真是神奇
        while row<m-1 and col>=0:
            if target==matrix[row][col]:
                return True
            if target>matrix[row][col]:
                row+=1
            else:
                col-=1
        
